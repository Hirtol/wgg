{
    "name": "wgg-frontend",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "predev": "cargo run -p wgg_schema_gen -- ./gen/",
        "dev": "conc \"vite dev --host\" \"graphql-codegen --config codegen.yml --watch\"",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "codegen": "graphql-codegen --config codegen.yml"
    },
    "devDependencies": {
        "@brainandbones/skeleton": "^0.56.2",
        "@graphql-codegen/add": "^3.2.1",
        "@graphql-codegen/cli": "2.13.3",
        "@graphql-codegen/typed-document-node": "^2.3.3",
        "@graphql-codegen/typescript": "2.7.3",
        "@graphql-codegen/typescript-operations": "^2.5.3",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@playwright/test": "^1.25.0",
        "@popperjs/core": "^2.11.6",
        "@sveltejs/adapter-static": "^1.0.0-next.43",
        "@sveltejs/kit": "next",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "autoprefixer": "^10.4.7",
        "carbon-icons-svelte": "^11.3.0",
        "carbon-preprocess-svelte": "^0.9.1",
        "classnames": "^2.3.2",
        "concurrently": "^7.4.0",
        "eslint": "^8.16.0",
        "svelte-tiny-virtual-list": "^2.0.5",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "flowbite": "^1.5.3",
        "flowbite-svelte": "^0.27.5",
        "graphql": "^15.8.0",
        "postcss": "^8.4.14",
        "postcss-lightningcss": "^0.6.0",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "svelte": "^3.44.0",
        "svelte-check": "^2.7.1",
        "svelte-preprocess": "^4.10.7",
        "tailwindcss": "^3.1.5",
        "tslib": "^2.3.1",
        "typescript": "^4.7.4",
        "vite": "^3.1.0"
    },
    "type": "module",
    "dependencies": {
        "@types/marked": "^4.0.7",
        "@urql/exchange-graphcache": "^5.0.1",
        "@urql/exchange-request-policy": "^1.0.0",
        "@urql/svelte": "^3.0.1",
        "marked": "^4.1.1"
    }
}
